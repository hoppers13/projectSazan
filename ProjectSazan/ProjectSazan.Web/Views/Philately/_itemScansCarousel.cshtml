<div id="scan-carousel" class="carousel slide" data-ride="carousel" data-interval="6000">
	<ol  class="carousel-indicators">
		<li v-for="(scan, index) in Scans" data-target="#scan-carousel" :data-slide-to="index" class="active"></li>		
	</ol>
	<div class="carousel-inner" role="listbox">
		<div v-for="(scan, index) in Scans" class="item active">
			<img :src="scan.Image" :alt="scan.Caption" class="img-responsive" />			
		</div>
	</div>
	@*<a class="left carousel-control" href="#scan-carousel" role="button" data-slide="prev">
